{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-chrome",
        "request": "launch",
        "name": "Vue.js Chrome",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}/frontend",
        "breakOnLoad": true,
        "sourceMapPathOverrides": {
          "webpack:///./~/*": "${webRoot}/node_modules/*",
          "webpack:///./src/*": "${webRoot}/*"
        }
      }
    ]
  }
  